{
  "cli": {
    "version": ">= 3.15.0"
  },
  "build": {
    "development": {
      "channel": "development",
      "developmentClient": false,
      "distribution": "internal",
      "ios": {
        "distribution": "internal"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
       "ENVIRONMENT":"dev"
      }
    },
    "test": {
      "distribution": "internal",
      "ios": {
        "distribution": "internal"
      },
      "android": {
        "buildType": "apk"
      },
      "env": {
        "ENVIRONMENT":"test"
       }
    },
    "production": {
      "channel": "production",
      "ios": {
        "distribution": "internal"
      },
      "android": {
        "buildType": "app-bundle"
      },
      "env": {
        "ENVIRONMENT":"prod"
       }
    }
  },
  "submit": {
    "production": {}
  }
}